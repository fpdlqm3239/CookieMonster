package xyz.itwill.dto;

/*
이름       		널?       		유형               
-------- 		-------- 		------------- 
no       		NOT NULL  		NUMBER 
email    		NOT NULL		VARCHAR2(50) 
pw       		NOT NULL		VARCHAR2(200) 
status     		NOT NULL		VARCHAR2(50) 
name     		NOT NULL		VARCHAR2(50) 
phone    		NOT NULL		VARCHAR2(13) 
birth          					VARCHAR2(30)
main_address   					VARCHAR2(100)
detail_address 					VARCHAR2(50)
zipcode        					VARCHAR2(5)
join_date      					DATE
last_join_date      			DATE

*/

public class MemberDTO {
	
	private int no;
	private String email;
	private String pw;
	private String status;
	private String name;
	private String phone;
	private String birth;
	private String mainAddress;
	private String detailAddress;
	private String zipcode;
	private String joinDate;
	private String lastJoinDate;
	
	



	public MemberDTO() {
		// TODO Auto-generated constructor stub
	}
	
	

	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPw() {
		return pw;
	}

	public void setPw(String pw) {
		this.pw = pw;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getBirth() {
		return birth;
	}

	public void setBirth(String birth) {
		this.birth = birth;
	}

	public String getMainAddress() {
		return mainAddress;
	}

	public void setMainAddress(String mainAddress) {
		this.mainAddress = mainAddress;
	}

	public String getDetailAddress() {
		return detailAddress;
	}

	public void setDetailAddress(String detailAddress) {
		this.detailAddress = detailAddress;
	}

	public String getZipcode() {
		return zipcode;
	}

	public void setZipcode(String zipcode) {
		this.zipcode = zipcode;
	}

	public String getJoinDate() {
		return joinDate;
	}

	public void setJoinDate(String joinDate) {
		this.joinDate = joinDate;
	}
	
	
	public String getLastJoinDate() {
		return lastJoinDate;
	}

	public void setLastJoinDate(String lastJoinDate) {
		this.lastJoinDate = lastJoinDate;
	}

	
	
	
	
	
	
	
	
	
	
	
}
